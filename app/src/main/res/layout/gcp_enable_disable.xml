<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:gravity="end"
    android:orientation="vertical">

    <include
        android:id="@+id/include2"
        layout="@layout/app_bar_back_only" />

    <TextView
        android:id="@+id/airprint"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/include2"
        android:gravity="center_vertical|center_horizontal"
        android:text="GCP Enable/Disable"
        android:textSize="20sp" />

    <RelativeLayout
        android:id="@+id/relativeLayout6"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/airprint"
        android:background="#313131"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView28"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:text="Google Cloud Print"
            android:textSize="15sp" />

        <ToggleButton
            android:id="@+id/toggleButton"
            style="@style/Widget.Button.Toggle"
            android:layout_width="70dp"
            android:layout_height="25dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:background="@drawable/toggle_selector"
            android:gravity="center_vertical"
            android:textOff=""
            android:textOn="" />

        <TextView
            android:id="@+id/tvState"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/textView28"
            android:layout_marginRight="10dp"
            android:layout_toLeftOf="@+id/toggleButton"
            android:layout_toStartOf="@+id/toggleButton"
            android:text="Disable"
            android:textSize="15sp" />

    </RelativeLayout>

    <TextView
        android:id="@+id/save_setting"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_below="@+id/relativeLayout6"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:background="@drawable/oval_background"
        android:gravity="center_vertical|center_horizontal"
        android:text="Save Settings"
        android:textSize="15sp"
        android:textAllCaps="false"/>
</RelativeLayout>