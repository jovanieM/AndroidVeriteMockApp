<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context="com.cebusqa.kodakverite.HM10_000">



    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="150dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout_printer"
        android:background="#000000">

        <include
            layout="@layout/app_bar_home"
            android:id="@+id/toolbar" />

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/setting_icon"
            android:background="#000000"
            android:src="@mipmap/printer1"
            android:longClickable="true"
            android:layout_gravity="center_horizontal" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/linearLayout_printer"
        android:id="@+id/linearLayout_printername"
        android:background="#00000000"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/printer_name"
            android:background="#000000"
            android:layout_gravity="center_horizontal|top"
            android:src="@mipmap/kodak_name" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:id="@+id/linearLayout_ecomode"
        android:layout_below="@+id/linearLayout_printername"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" >

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/ecomode"
            android:src="@mipmap/ecomode_off"
            android:background="#000000"
            android:layout_gravity="center"
            android:layout_below="@+id/linearLayout_ecomode"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
    </LinearLayout>

    <ScrollView
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
        android:layout_below="@+id/linearLayout_ecomode"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/linearLayout_ecomode"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#000000"
        android:scrollIndicators="top">


        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="166dp"
                android:weightSum="1"
                android:id="@+id/linearLayout_first"
                android:background="#000000"
            android:gravity="center_horizontal">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:id="@+id/inklevel"
                android:background="#000000"
                android:src="@mipmap/inklevel"
                android:layout_weight="0.49" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:id="@+id/photoprint"
                android:layout_weight="0.43"
                android:background="#000000"
                android:src="@mipmap/photoprint" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="166dp"
            android:weightSum="1"
            android:id="@+id/linearLayout_second"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:gravity="center_horizontal">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/copy"
                android:layout_weight="0.49"
                android:src="@mipmap/copy"
                android:background="#000000" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/scandocument"
                android:layout_weight="0.43"
                android:layout_gravity="left"
                android:src="@mipmap/scandocument"
                android:background="#000000" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="166dp"
            android:weightSum="1"
            android:id="@+id/linearLayout2"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:gravity="center_horizontal" >

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/scanphoto"
                android:layout_weight="0.49"
                android:src="@mipmap/scanphoto"
                android:background="#000000" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton3"
                android:layout_weight="0.43"
                android:layout_gravity="left"
                android:background="#000000"
                android:src="@mipmap/dummy" />
        </LinearLayout>


    </LinearLayout>

</ScrollView>

</RelativeLayout>
